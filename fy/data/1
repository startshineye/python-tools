class SysParameters(Structure):
    _fields_ = [
        ("windowLen", c_float * 15),
        ("freqBins", c_float * 15),
        ("csThd", c_float * 15),
        ("corrThd", c_float * 15),
        ("sigma_A", c_float * 15),
        ("sigma_V", c_float * 15),
        ("sigma_F", c_float),
        ("sigma_T", c_float),
        ("timeThd", c_float),
        ("nFamilyMax", c_int),
        ("nFamilyMin", c_int),
        ("ThresholdDistance", c_float),
        ("VelocityMax", c_float),
        ("VelocityMin", c_float),
        ("freqHighLim", c_float * 15),
        ("freqLowLim", c_float * 15),
        ("overlapRate", c_float),
    ]